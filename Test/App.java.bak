import java.applet.Applet;
import java.awt.*;

public class App extends Applet
{
	Font f = new Font("TimesRoman",Font.BOLD,40);
	String name;
	public void init()
	{
		name = "Alok Panda";
	}
	public void start()
	{
	}
	public void paint(Graphics g)
	{
		g.setFont(f);
		g.setColor(Color.blue);
		g.drawString(name,50,20);
	}
}